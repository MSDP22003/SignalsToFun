# need to install pyfirmata
# In arduino IDE go to file, examples, firmata, standard firmata, upload to arduino
import pyfirmata
import time

board = pyfirmata.Arduino('COM5')
it = pyfirmata.util.Iterator(board)
it.start()

analog_input = board.get_pin('a:0:i')

while True:
    analog_value = analog_input.read()
    print(analog_value)
    time.sleep(0.1)
